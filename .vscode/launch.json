{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      //"label": "Compile",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}" ,
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "E:\\Install_Path\\MinGW-W64 GCC\\bin\\gdb.exe",
      "preLaunchTask": "gcc",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
  ]
}